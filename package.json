{
  "name": "changelog",
  "version": "1.1.1",
  "description": "generate changelog auto",
  "main": "index.js",
  "scripts": {
    "release": "standard-version",
    "release:minor": "standard-version --relase-as minor",
    "release-patch": "standard-version --relase-as patch",
    "release-major": "standard-version --release-as major",
    "commit-msg": "commit-msg",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samruddhipwr/Changelog.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/samruddhipwr/Changelog/issues"
  },
  "homepage": "https://github.com/samruddhipwr/Changelog#readme",
  "devDependencies": {
    "husky": "^7.0.0",
    "standard-version": "^9.3.2"
  },
  "dependencies": {
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4"
  }
}
